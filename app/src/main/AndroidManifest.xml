<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.heriana.javaaplikasi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/firebase_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/firebase_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.LoginActivity"
            android:label="Authentication">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.RegistrationActivity"
            android:label="Authentication" />
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.MenuMateriActivity" />
        <activity android:name=".Activities.LogoutActivity" />
        <activity android:name=".Activities.MateriJavaProgramming" />
        <activity android:name=".Activities.MateriJavaFundamental" />
        <activity android:name=".Activities.SoalActivity" />
        <activity android:name=".Activities.CompilerActivity" />
        <activity android:name=".Activities.SkorActivity"/>
    </application>

</manifest>